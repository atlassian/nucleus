{
  "name": "nucleus-server",
  "version": "0.0.2",
  "description": "Nucleus update server",
  "main": "lib/index.js",
  "bin": {
    "nucleus": "./lib/index.js"
  },
  "scripts": {
    "build-server": "tsc",
    "build-fe-prod": "webpack --config webpack.production.config.js",
    "clean": "rm -rf lib && rm -rf public_out",
    "commit": "git-cz",
    "dev": "concurrently --raw \"npm run start-fe\" \"npm run start-server-dev\" \"npm run start-static\"",
    "lint": "tslint -c tslint.json -p tsconfig.json --type-check && tslint -c tslint.json -p tsconfig.public.json --type-check",
    "prepublish": "npm run clean && npm run lint && npm run build-server && npm run build-fe-prod",
    "start": "npm run start-server",
    "start-fe": "webpack-dev-server --progress --profile --colors --quiet --open",
    "start-server": "node lib/index.js",
    "start-server-dev": "nodemon --watch src --exec \"npm run build-server && cross-env DEBUG=nucleus* npm run start-server\" -e ts",
    "start-static": "mkdir -p .files && serve .files --port 9999",
    "test": "mocha --compilers ts:ts-node/register src/**/__spec__/*_spec.ts"
  },
  "author": "Samuel Attard",
  "license": "Apache-2.0",
  "dependencies": {
    "aws-sdk": "^2.119.0",
    "body-parser": "^1.17.2",
    "compression": "^1.7.1",
    "connect-redis": "^3.3.2",
    "debug": "^2.6.8",
    "express": "^4.15.3",
    "express-fileupload": "^0.1.3",
    "express-form-data": "^2.0.0",
    "express-session": "^1.15.3",
    "fs-extra": "^3.0.1",
    "hat": "^0.0.3",
    "mysql2": "^1.4.2",
    "node-fetch": "^1.7.3",
    "node-rsa": "^0.4.2",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "passport-http": "^0.3.0",
    "passport-openid": "^0.4.0",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "pify": "^3.0.0",
    "reflect-metadata": "^0.1.10",
    "semver": "^5.4.1",
    "sequelize": "^4.11.1",
    "sequelize-typescript": "^0.5.0",
    "sqlite3": "^3.1.12",
    "tedious": "^2.0.0"
  },
  "devDependencies": {
    "@atlaskit/avatar": "^4.0.5",
    "@atlaskit/banner": "^4.0.1",
    "@atlaskit/button": "^5.4.0",
    "@atlaskit/css-reset": "^1.1.4",
    "@atlaskit/droplist": "^4.0.1",
    "@atlaskit/field-base": "^8.1.1",
    "@atlaskit/field-text": "^4.0.1",
    "@atlaskit/icon": "^6.6.0",
    "@atlaskit/modal-dialog": "^2.6.0",
    "@atlaskit/multi-select": "^7.1.3",
    "@atlaskit/navigation": "^13.0.2",
    "@atlaskit/spinner": "^2.2.3",
    "@atlaskit/tabs": "^4.0.0",
    "@atlaskit/tooltip": "^6.2.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/body-parser": "^1.16.3",
    "@types/chai": "^4.0.4",
    "@types/compression": "^0.0.34",
    "@types/connect-redis": "^0.0.6",
    "@types/debug": "0.0.29",
    "@types/express": "^4.0.35",
    "@types/express-session": "^1.15.0",
    "@types/fs-extra": "^3.0.2",
    "@types/mocha": "^2.2.43",
    "@types/node": "^7.0.22",
    "@types/node-fetch": "^1.6.7",
    "@types/passport": "^0.3.3",
    "@types/passport-github": "^1.1.0",
    "@types/passport-http": "^0.3.2",
    "@types/pify": "0.0.28",
    "@types/react": "^16.0.20",
    "@types/react-dom": "^16.0.2",
    "@types/react-hot-loader": "^3.0.1",
    "@types/react-redux": "^4.4.41",
    "@types/react-router": "^2.0.0",
    "@types/redux": "^3.6.0",
    "@types/semver": "^5.4.0",
    "@types/sinon": "^2.3.5",
    "@types/webpack-env": "^1.13.0",
    "awesome-typescript-loader": "^3.1.3",
    "chai": "^4.1.2",
    "commitizen": "^2.9.6",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "cz-customizable": "^5.2.0",
